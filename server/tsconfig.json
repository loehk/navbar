{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "rootDir": "./src",
    "outDir": "./dist",
    "noEmitOnError": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "typeRoots":["./node_modules/@types", "./types"]
  },
  "include": ["src", "types"],
  "typeRoots":["./node_modules/@types", "./types"]


}
